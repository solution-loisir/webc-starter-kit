<div class="series-listing">
  <p><big><span @text="name" webc:nokeep></span> series!</big></p>
  <ul role="list">
    <script webc:type="js">
      collections.series[name]
        .map((series) => `<li><a href="${series.page.url}">${series.data.title}</a></li>`)
        .join("\n");
    </script>
  </ul>
</div>