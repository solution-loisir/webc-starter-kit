<p>&copy; <span webc:nokeep @html="copyrightYear(firstInProduction)"></span> <span webc:nokeep @text="orgName"></span></p>

<div @html="number('3') - 2"></div>

<ul @html="loop(collections.all, (item) => `
  <li>${item.data.title}</li>
  <ul>
    <li>${item.data.description}</li>
    <li><a href='${item.page.url}'>${item.page.fileSlug}</a></li>
  </ul>`
)">
</ul>