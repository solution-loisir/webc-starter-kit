<script webc:type="render" webc:is="template">
  function copyright() {
    const initialYear = this.firstInProduction ? new Date(this.firstInProduction).getFullYear() : new Date().getFullYear();
    const currentYear = new Date().getFullYear();
    const yearStatement = (currentYear - initialYear) > 0 ? `${initialYear}-${currentYear}` : initialYear;
    return `<p>&copy; ${yearStatement} ${this.orgName}</p>`;
  };
</script>