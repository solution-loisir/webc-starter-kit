<script webc:type="js" webc:root="override">
  const initialYear = firstInProduction ? new Date(firstInProduction).getFullYear() : new Date().getFullYear();
  const currentYear = new Date().getFullYear();
  const yearStatement = (currentYear - initialYear) > 0 ? `${initialYear}-${currentYear}` : initialYear;

  `<p>&copy; ${yearStatement} ${orgName}</p>`;
</script>